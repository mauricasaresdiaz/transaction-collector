{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "indent": ["error", 4],
        "max-classes-per-file": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "no-undef": "off"
    }
}
